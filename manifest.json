{
  "name": "File Styled Directory",
  "description": "Extension that apply css styles at file directory.",
  "author": "Matheus Henrique dos Santos Almeida",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "images/logo.png"
  },
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["file:///*"],
      "css": [
        "styles/main.css",
        "styles/data-table.css",
        "styles/button-toggle-theme.css",
        "styles/button-previous-folder.css"
      ],
      "js": [
        "scripts/buttonToggleTheme.js",
        "scripts/buttonPreviousFolder.js"
      ]
    } 
  ]
}